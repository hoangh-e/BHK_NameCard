        );
        Set(varShowUserForm, false);
        Set(varFormMode, "Add");
        Set(varEditingUser, Blank());
        Set(varSelectedUser, Blank());
        Set(varDeleteConfirm, false);
        Set(varLoadingUsers, false);
        Set(varErrorMessage, "")
