                        BorderColor: =Colors.Primary
                        BorderThickness: =If(varLoadingUsers, 2, 1)
                        Color: =Styles.Button.Primary.Color
                        DisplayMode: =If(varLoadingUsers, DisplayMode.Disabled, DisplayMode.Edit)
